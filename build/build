#!/bin/bash

set -e

if [ "$(ls -A /tachyon)" ]; then
  cd /tachyon 
  if [ ! -z "$SHA1_OR_REF" ]; then
    git checkout master
    if [ `git branch --list dev` ]; then
      git branch -D dev
    fi
    git checkout -b dev $SHA1_OR_REF
  fi
else
  echo "/tachyon is empty :("
  exit 1
fi

exec mvn -DskipTests install
